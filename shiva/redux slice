import { createSlice } from '@reduxjs/toolkit';

const initialState = {
  data: {},
};
const branchesSlice = createSlice({
  name: 'branches',
  initialState,
  reducers: {
    getDataSuccess: (state, { payload }) => {
      return 1;
    },
  },
});

export const { getDataSuccess } = branchesSlice.actions;
export const { reducer } = branchesSlice;







